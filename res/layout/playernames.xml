<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/RelativeLayout01"
	android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" android:paddingRight="5dp" android:paddingTop="5dp" android:paddingLeft="5dp" android:paddingBottom="10dp">
	<TextView android:textStyle="bold" android:layout_height="wrap_content" android:text="@string/maintab_menu_people" android:textSize="25sp" android:id="@+id/title" android:layout_alignParentTop="true" android:layout_alignParentLeft="true" android:layout_width="fill_parent" android:textColor="@color/textTitleLogs" android:gravity="center_horizontal" android:padding="5dp" android:layout_toLeftOf="@+id/OkButton" android:paddingBottom="5dp"></TextView>
	<ImageView android:src="@drawable/divider_horizontal_dark" android:layout_height="wrap_content" android:id="@+id/imageView1" android:layout_width="fill_parent" android:layout_below="@+id/OkButton" android:paddingBottom="10dp"></ImageView>
	<ListView android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/listView" android:layout_below="@+id/imageView1"></ListView>
	<ImageButton android:src="@drawable/btn_no" android:layout_height="wrap_content" android:id="@+id/OkButton" android:paddingBottom="5dp" android:layout_width="wrap_content" android:layout_alignParentTop="true" android:layout_alignParentRight="true" android:paddingTop="5dp" android:paddingRight="5dp" android:background="@drawable/bgempty"></ImageButton>
</RelativeLayout>
